{
  "name": "react-typescript-starter",
  "version": "0.2.0",
  "private": true,
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts-ts": "3.1.0"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "lint": "tslint src/**/*",
    "test": "npm run lint && jest --config jest.config.js --env=jsdom",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage",
    "eject": "react-scripts-ts eject"
  },
  "jest-junit": {
    "output": "./reports/tests/junit/junit.xml",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "usePathForSuiteName": "true"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "^13.11.1",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "babel-jest": "^23.6.0",
    "cross-env": "^5.1.4",
    "date-fns": "^1.29.0",
    "jest": "^23.6.0",
    "jest-css": "^1.0.1",
    "jest-file": "^1.0.0",
    "jest-junit": "^3.7.0",
    "ts-jest": "^22.4.4",
    "tslint-config-airbnb": "^5.8.0",
    "typescript": "^3.8.3"
  }
}
